    %a{href: "https://fonts.googleapis.com/icon?family=Material+Icons" rel: "stylesheet"}
    %a{type: "text/css" rel: "stylesheet" href:"css/materialize.min.css"  media: "screen,projection"}

    %body
    %script{type:"text/javascript" src: "js/materialize.min.js"}
